*******Scanner*******
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Camera Photo Capture</title>
    <style>
        body {
            font-family: Arial, sans-serif;
            text-align: center;
            margin: 0;
            padding: 20px;
            background-color: yellow; /* Set the background color to yellow */
            color: blue; /* Set text color to blue */
        }

        #camera-feed {
            margin: 20px 0;
        }

        #capture-button {
            background-color: blue; /* Set button background color to blue */
            color: white; /* Set button text color to white */
            padding: 10px 20px;
            border: none;
            cursor: pointer;
        }

        #download-link {
            color: blue; /* Set link color to blue */
            text-decoration: none;
        }
    </style>
</head>
<body>
    <h1>Camera Photo Capture</h1>
    <div id="camera-feed">
        <video id="video" autoplay playsinline></video>
    </div>
    <button id="capture-button">Capture Photo</button>
    <canvas id="canvas" style="display: none;"></canvas>
    <a id="download-link" style="display: none;" download="photo.jpg">Download Photo</a>
    <script>
        const video = document.getElementById('video');
        const captureButton = document.getElementById('capture-button');
        const canvas = document.getElementById('canvas');
        const downloadLink = document.getElementById('download-link');

        async function initCamera() {
            try {
                const stream = await navigator.mediaDevices.getUserMedia({ video: true });
                video.srcObject = stream;
            } catch (error) {
                console.error('Error accessing the camera: ' + error.message);
            }
        }

        function capturePhoto() {
            canvas.width = video.videoWidth;
            canvas.height = video.videoHeight;
            canvas.getContext('2d').drawImage(video, 0, 0, canvas.width, canvas.height);
            downloadLink.href = canvas.toDataURL('image/jpeg');
            downloadLink.style.display = 'block';
        }

        initCamera();

        captureButton.addEventListener('click', capturePhoto);
    </script>
</body>
</html>
















